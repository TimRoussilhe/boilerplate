{
  "name": "Boilerplate",
  "description": "Boilerplate framework",
  "version": "0.0.2",
  "private": true,
  "author": "Tim Roussilhe",
  "contributors": [
    "Tim Roussilhe"
  ],
  "app": {},
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "helmet": "^2.3.0",
    "mobile-detect": "^1.3.3",
    "ua-parser-js": "^0.7.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "autoprefixer-stylus": "^0.11.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "backbone": "^1.3.3",
    "browser-sync": "^2.17.3",
    "concurrently": "^3.1.0",
    "connect-livereload": "^0.6.0",
    "customizr": "^1.0.0-alpha",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "exports-loader": "^0.6.3",
    "fastclick": "^1.0.6",
    "gsap": "^1.19.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-connect-php": "^1.0.1",
    "gulp-csso": "^2.0.0",
    "gulp-files-to-json": "^0.2.1",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-stylus": "^2.6.0",
    "gulp-svgmin": "^1.2.3",
    "gulp-watch": "^4.3.11",
    "gulp.spritesmith": "^6.2.1",
    "handlebars": "^4.0.5",
    "imports-loader": "^0.6.5",
    "is_js": "^0.9.0",
    "json-loader": "^0.5.4",
    "merge-stream": "^1.0.0",
    "npm-zepto": "^1.1.7",
    "poststylus": "^0.2.3",
    "run-sequence": "^1.2.2",
    "stylelint": "^7.9.0",
    "stylus": "^0.54.5",
    "twig-loader": "^0.3.0",
    "underscore": "^1.8.3",
    "vinyl-buffer": "^1.0.0",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "build:js": "ENV_PROD=0 webpack",
    "dist:sass": "node-sass --include-path scss ./src/css/app.scss ./build/assets/css/app.min.css",
    "dist:autoprefixer": "postcss --use autoprefixer -c autoprefixer-options.json ./build/assets/css/app.sassmin.css --replace",
    "dist:js": "ENV_PROD=1 webpack",
    "lint:css": "stylelint ./build/assets/css/*.css",
    "lint:js": "eslint ./src/scripts/project",
    "watch:js": "ENV_PROD=0 webpack --progress --colors --watch --cache",
    "svgs-to-jsons": "gulp svgs-to-jsons",
    "server": "php -S localhost:8000 -t public/",
    "browsersync": "gulp browserSync",
    "build": "gulp build",
    "dev": "concurrently -p \"[{name}]\" -n \"WEBPACK\" -c \"bgBlue.bold,bgRed.bold\" \"npm run watch:js\"",
    "start": "npm run server & npm run browsersync & npm run dev",
    "deploy": "npm run dist:sass && npm run dist:autoprefixer && npm run dist:js && npm run lint:js && npm run build "
  }
}
